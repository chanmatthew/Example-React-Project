{
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:import/warnings",
    "prettier"
  ],
  "plugins": [ "compat", "import"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "compat/compat": 2,
    "no-console": 1,
    "semi": 2
  },
  "env": {
    "node": true,
    "browser": true
  },
  "settings": {
    "polyfills": ["fetch"]
  }
}