{
  "name": "react-stack",
  "version": "1.0.0",
  "description": "Example project using React technology stack",
  "main": "index.js",
  "scripts": {
    "lint": "eslint '**/*.js'",
    "lint:watch": "esw -w '**/*.js'",
    "run-dist": "node dist/index",
    "run-index": "babel-node index",
    "pretranspile": "rimraf dist && mkdir dist",
    "transpile": "babel index.js --out-file dist/index.js",
    "predev": "echo \"pre dev\"",
    "dev": "echo \"dev\"",
    "postdev": "echo \"post dev\"",
    "prestart": "echo \"pre start\"",
    "start": "node index",
    "poststart": "echo \"post start\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "React.js",
    "npm",
    "yarn",
    "node"
  ],
  "author": "Matthew Chan",
  "license": "ISC",
  "browsersList": [
    "> 1%"
  ],
  "dependencies": {
    "color": "^3.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.9.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-compat": "^2.6.3",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-watch": "^4.0.2",
    "husky": "^1.2.0",
    "prettier": "^1.15.2",
    "rimraf": "^2.6.2"
  }
}
